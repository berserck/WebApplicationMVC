@model IEnumerable<WebApplicationMVC.Models.RestaurantListViewModel>

@{
    ViewBag.Title = "Home Page";
}

<h1>Restaurants</h1>
@foreach (var item in Model)
{
    <div>
        <h4>@item.Name</h4>
        <div>@item.City, @item.Country</div>
        <div>
            Reviews: @item.CountOfReviews
        </div>
        <hr />
    </div>
}